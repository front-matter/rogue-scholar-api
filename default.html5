<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml" lang="$lang$" xml:lang="$lang$"$if(dir)$ dir="$dir$"$endif$>
<head>
  <meta charset="utf-8" />
  <meta name="generator" content="pandoc" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes" />
$for(author-meta)$
  <meta name="author" content="$author-meta$" />
$endfor$
$if(date-meta)$
  <meta name="dcterms.date" content="$date-meta$" />
$endif$
$if(keywords)$
  <meta name="keywords" content="$for(keywords)$$keywords$$sep$, $endfor$" />
$endif$
$if(description-meta)$
  <meta name="description" content="$description-meta$" />
$endif$
  <title>$if(title-prefix)$$title-prefix$ â€“ $endif$$pagetitle$</title>
  <style>
    $styles.html()$
  </style>
$for(css)$
  <link rel="stylesheet" href="$css$" />
$endfor$
$for(header-includes)$
  $header-includes$
$endfor$
$if(math)$
$if(mathjax)$
  <script src="https://polyfill.io/v3/polyfill.min.js?features=es6"></script>
$endif$
  $math$
$endif$
</head>
<body>
$for(include-before)$
$include-before$
$endfor$
<section class="front-matter">
  $if(title)$
    <h1 class="title">$title$</h1>
    $if(subtitle)$
      <p class="subtitle">$subtitle$</p>
    $endif$
  $endif$
  <span class="header">$blog_name$</span>
  <p class="author">
  $if(author/allbutlast)$
    $for(author/allbutlast)$
      $if(it.orcid)$
        <span>${it.name}<a href="${it.orcid}"><img alt="ORCID logo" src="orcid.svg" style="margin-left: 2px; vertical-align: -1px" /></a></span>$else$<span>${it.name}</span>$endif$$sep$, $endfor$
    $for(author/last)$
      $if(it.orcid)$
        and <span>${it.name}<a href="${it.orcid}"><img alt="ORCID logo" src="orcid.svg" style="margin-left: 2px; vertical-align: -1px" /></a></span>
      $else$
        and <span>${it.name}</span>
      $endif$
    $endfor$
  $else$
    $for(author)$
      $if(it.orcid)$
        <span>${it.name}<a href="${it.orcid}"><img alt="ORCID logo" src="orcid.svg" style="margin-left: 2px; vertical-align: -1px" /></a></span>
      $else$
        <span>${it.name}</span>
      $endif$
    $endfor$
  $endif$
  </p>
  $if(date)$
    <div class="date">
      Published $date$
    </div>
  $endif$
  $if(citation)$
    <div class="citation">
      <div class="citation-title">Citation</div>
      $citation$
    </div>
  $endif$
  $if(keywords)$
    <div class=""keywords>
      <div class="keywords-title">Keywords</div>
      $keywords[, ]$
    </div>
  $endif$
  $if(abstract)$
    <div class="abstract">
      <div class="abstract-title">$abstract-title$</div>
        $abstract$
    </div>
  $endif$
  $if(image)$
    <img class="feature-image" alt="Feature Image" src="$image$" />
  $endif$
  $if(license)$
  <div class="rights">
    <div class="rights-title">Copyright</div>
    $license.text$ Distributed under the terms of the <a href="$license.link$">Creative Commons Attribution 4.0 International License</a>,
    which permits unrestricted use, distribution, and reproduction in any medium, provided the original author and 
    source are credited.
  </div>
$endif$
</section>
$if(toc)$
<nav id="$idprefix$TOC" role="doc-toc">
$if(toc-title)$
<h2 id="$idprefix$toc-title">$toc-title$</h2>
$endif$
$table-of-contents$
</nav>
$endif$
$body$
$for(include-after)$
$include-after$
$endfor$
</body>
</html>